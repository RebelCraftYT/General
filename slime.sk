command /setspawn [<text>]:
    permission: op
    trigger:
        arg is not set:
            set {spawn} to location of block at player
            send "&aSet spawn to your location!" to player
        arg is "hubfromspider":
            set {hubfromspider} to location of block at location of player
            send "&aSet Spider-Hub spawn to your location!" to player
        arg is "spiderfromhub":
            set {spiderfromhub} to location of block at location of player
            send "&aSet Hub-Spider spawn to your location!" to player
        arg is "endfromspider":
            set {endfromspider} to location of block at location of player
            send "&aSet Spider-End spawn to your location!" to player
        arg is "spiderfromend":
            set {spiderfromend} to location of block at location of player
            send "&aSet End-Spider spawn to your location!" to player
        arg is "netherfromspider":
            set {netherfromspider} to location of block at location of player
            send "&aSet Spider-Nether spawn to your location!" to player
        arg is "spiderfromnether":
            set {spiderfromnether} to location of block at location of player
            send "&aSet Nether-Spider spawn to your location!" to player
        arg is "forestfromhub":
            set {forestfromhub} to location of block at location of player
            send "&aSet Spider-Nether spawn to your location!" to player
        arg is "hubfromforest":
            set {hubfromforest} to location of block at location of player
            send "&aSet Nether-Spider spawn to your location!" to player
        arg is "goldfromhub":
            set {goldfromhub} to location of block at location of player
            send "&aSet Hub-Gold spawn to your location!" to player
        arg is "hubfromgold":
            set {hubfromgold} to location of block at location of player
            send "&aSet Gold-Hub spawn to your location!" to player
        arg is "deepfromgold":
            set {deepfromgold} to location of block at location of player
            send "&aSet Gold-Deep spawn to your location!" to player
        arg is "goldfromdeep":
            set {goldfromdeep} to location of block at location of player
            send "&aSet Deep-Gold spawn to your location!" to player
        arg is "farmfromhub":
