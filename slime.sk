            send "&aSet Desert-Farm spawn to your location!" to player

command /hub:
    trigger:
        make player execute command "/server hub"

command /spawn:
    trigger:
        send player title "&aStand Still" with subtitle "&aTeleporting in &e5 Seconds!" for 5 seconds
        set {_t} to location of block at location of player
        wait 5 seconds
        set {_b} to location of block at location of player
        {_t} = {_b}:
            teleport player to {spawn}
            send "&aTeleported to spawn!" to player
            stop
        else:
            send "&cYou moved! Teleportation Canceled." to player

command /addabove:
    trigger:
        loop blocks in radius 3 around player:
            loop-block is slime block:
                add location of loop-block to {above::*}

command /addbelow:
    trigger:
        loop blocks in radius 3 around player:
            loop-block is slime block:
                add location of loop-block to {below::*}

on step on slime block:
    "%region at player%" contains "wood":
        {below::*} contains location of block below player:
            push player up at speed 2 
            wait 10 ticks
            push player down at speed 1
            push player forward at speed 1
        {above::*} contains location of block below player:
            push player forward at speed 1
        stop
    play sound "BLOCK.PORTAL.TRAVEL" with volume 10 and pitch 1.5
    "%region at player%" contains "hubspider":
        execute console command "/speed walk 0 %player%"
        apply slowness of tier 100 to player for 5 seconds
        send title "&aTeleporting..." to player
        wait 5 seconds
        teleport player to {hubfromspider}
        send title "&aTeleported to Hub" to player
        execute console command "/speed walk 1 %player%"
        stop
